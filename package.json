{
  "name": "karin-plugin-mys-core",
  "version": "1.0.16",
  "author": "babanbang",
  "type": "module",
  "description": "karin-plugin-mys-core",
  "homepage": "https://github.com/Karin-Mys-Plugins/karin-plugin-mys-core",
  "bugs": {
    "url": "https://github.com/Karin-Mys-Plugins/karin-plugin-mys-core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Karin-Mys-Plugins/karin-plugin-mys-core.git"
  },
  "scripts": {
    "app": "node lib/app.js",
    "build": "npm run build:css && tsup",
    "build:css": "tailwindcss -i src/template/tailwind.css -o resources/styles/karin-plugin-mys-core.css",
    "dev": "cross-env EBV_FILE=\"development.env\" node --import tsx src/app.ts",
    "dev:css": "tailwindcss -i src/template/tailwind.css -o resources/styles/karin-plugin-mys-core.css --watch",
    "pub": "npm publish --access public"
  },
  "exports": {
    "./core": {
      "types": "./lib/core/index.d.ts",
      "development": "./src/core/index.ts",
      "default": "./lib/core/index.js"
    },
    "./config": {
      "types": "./lib/exports/config/index.d.ts",
      "development": "./src/exports/config/index.ts",
      "default": "./lib/exports/config/index.js"
    },
    "./database": {
      "types": "./lib/exports/database/index.d.ts",
      "development": "./src/exports/database/index.ts",
      "default": "./lib/exports/database/index.js"
    },
    "./mys": {
      "types": "./lib/exports/mys/index.d.ts",
      "development": "./src/exports/mys/index.ts",
      "default": "./lib/exports/mys/index.js"
    },
    "./render": {
      "types": "./lib/exports/render/index.d.ts",
      "development": "./src/exports/render/index.ts",
      "default": "./lib/exports/render/index.js"
    },
    "./utils": {
      "types": "./lib/exports/utils/index.d.ts",
      "development": "./src/exports/utils/index.ts",
      "default": "./lib/exports/utils/index.js"
    }
  },
  "main": "lib/index.js",
  "dependencies": {
    "md5": "^2.3.0",
    "qrcode": "^1.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@karinjs/plugin-puppeteer": "^1.1.2",
    "@types/express": "^5.0.1",
    "@types/lodash": "^4.17.16",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.17.8",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "^9.7.0",
    "neostandard": "^0.11.9",
    "node-karin": "^1.13.0",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.5.3"
  },
  "karin": {
    "main": "src/index.ts",
    "apps": [
      "lib/apps"
    ],
    "ts-apps": [
      "src/apps"
    ],
    "web": "./lib/web.config.js",
    "ts-web": "./src/web.config.ts",
    "static": [
      "resources"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ]
  },
  "files": [
    "LICENSE",
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "resources",
    "!lib/app.js",
    "!lib/app.d.ts",
    "!lib/index.d.ts",
    "!lib/apps/**/*.d.ts"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
